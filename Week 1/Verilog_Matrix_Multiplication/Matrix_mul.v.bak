`timescale 1ns/1ps
module mul(clk,a,b,mul);
	input [8:0]a,b
	input clk;
	output reg [2:0][2:0]mul;
	reg sum;
	reg ra[2:0][2:0];
	reg rb[2:0][2:0];
	integer i,j,k;
	k=1'd0;
	for(i=0;i<3;i=i+1)
		begin
		for(j=0;j<3;j=j+1)
			begin
			ra[i][j] = a[k];
			rb[i][j] = b[k];
			end
		k = k+3;
		end
	always@(posedge clk)
		for(i=0;i<=2;i=i+1) 
			begin 
			for(j=0;j<=2;j=j+1) 
				begin 
				sum=1'b0; 
				for(k=0;k<=2;k=k+1) 
					begin 
						sum=sum + ra[i][k]*rb[k][j]; 
					end 
				mul[i][j]=sum; 
			end 
		end 